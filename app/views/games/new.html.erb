<div style="padding-top: 15px; padding-left: 30px; padding-right: 30px">
  <p><a class="btn btn-secondary" href="<%= games_path %>" role="button">Back</a></p>
  <h3> Creating games </h3>
  <%= form_for :game, url: games_path do |f|%>
    <p>
      Name of the game <br>
      <%= f.text_field :game_name%>
    </p>
    <p>
      <br>
      <%= f.submit %>
    </p>
  <% end %>
</div>
